{
  "name":"vip-video",
  "version":"0.0.1",
  "manifest_version":2,
  "description":"一个清爽模式观看全网视频的插件",
  "icons" : {
    "16":"image/icon16.png",
    "48":"image/icon48.png"
  },
  "browser_action":{
    "default_title": "Start.",
    "default_popup":"html/index.html"
  },
  "background": {
    "scripts": ["sample.js"]
  },
  "permissions":[
    "http://*/*",
    "https://*/*",
    "tabs",
    "contextMenus"
  ]
}